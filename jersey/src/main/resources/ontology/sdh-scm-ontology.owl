@prefix : <http://www.centeropenmiddleware.com/ontology/2015/scm#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.centeropenmiddleware.com/ontology/2015/scm> .

<http://www.centeropenmiddleware.com/ontology/2015/scm> rdf:type owl:Ontology ;
                                                        
                                                        rdfs:comment """This ontology models a part of the concepts involved in a SCM system. We have resctricted the ontoloy to the concepts and the relations needed by the system to calculate the following set of metrics:

(Global) Average commits per project
(User) Average commits per project
(Global) Average committers per project
(Global) Total commits
(Project) Time since last commit
(Project) Total commits"""@en ;
                                                        
                                                        owl:imports <http://usefulinc.com/ns/doap#> .


#################################################################
#
#    Object Properties
#
#################################################################


###  http://www.centeropenmiddleware.com/ontology/2015/scm#hasBranch

:hasBranch rdf:type owl:ObjectProperty ;
           
           rdfs:domain <http://usefulinc.com/ns/doap#Repository> ;
           
           rdfs:range :Branch .



###  http://www.centeropenmiddleware.com/ontology/2015/scm#hasCommit

:hasCommit rdf:type owl:ObjectProperty ;
           
           rdfs:range :Commit ;
           
           rdfs:domain :Merge .



###  http://www.centeropenmiddleware.com/ontology/2015/scm#isSourceOf

:isSourceOf rdf:type owl:ObjectProperty ;
            
            rdfs:range :Action ;
            
            rdfs:domain :Branch .



###  http://www.centeropenmiddleware.com/ontology/2015/scm#isTargetOf

:isTargetOf rdf:type owl:ObjectProperty ;
            
            rdfs:range :Action ;
            
            rdfs:domain :Branch .



###  http://www.centeropenmiddleware.com/ontology/2015/scm#mergeCommit

:mergeCommit rdf:type owl:ObjectProperty ;
             
             rdfs:range :Commit ;
             
             rdfs:domain :TrueMerge .



###  http://www.centeropenmiddleware.com/ontology/2015/scm#performedBy

:performedBy rdf:type owl:ObjectProperty ;
             
             rdfs:domain :Action ;
             
             rdfs:range <http://xmlns.com/foaf/0.1/Person> .





#################################################################
#
#    Data properties
#
#################################################################


###  http://usefulinc.com/ns/doap#name

<http://usefulinc.com/ns/doap#name> rdf:type owl:DatatypeProperty .



###  http://www.centeropenmiddleware.com/ontology/2015/scm#createdOn

:createdOn rdf:type owl:DatatypeProperty ;
           
           rdfs:range xsd:dateTime ;
           
           rdfs:domain [ rdf:type owl:Class ;
                         owl:unionOf ( <http://usefulinc.com/ns/doap#Repository>
                                       :Action
                                       :Branch
                                       <http://xmlns.com/foaf/0.1/Project>
                                     )
                       ] .





#################################################################
#
#    Classes
#
#################################################################


###  http://usefulinc.com/ns/doap#Repository

<http://usefulinc.com/ns/doap#Repository> rdf:type owl:Class ;
                                          
                                          rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                            owl:onProperty :createdOn ;
                                                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                            owl:onDataRange xsd:dateTime
                                                          ] ,
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty <http://usefulinc.com/ns/doap#name> ;
                                                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                            owl:onDataRange xsd:string
                                                          ] .



###  http://www.centeropenmiddleware.com/ontology/2015/scm#Action

:Action rdf:type owl:Class ;
        
        rdfs:subClassOf [ rdf:type owl:Restriction ;
                          owl:onProperty :performedBy ;
                          owl:onClass <http://xmlns.com/foaf/0.1/Person> ;
                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :createdOn ;
                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onDataRange xsd:dateTime
                        ] .



###  http://www.centeropenmiddleware.com/ontology/2015/scm#Branch

:Branch rdf:type owl:Class ;
        
        rdfs:subClassOf [ rdf:type owl:Restriction ;
                          owl:onProperty :createdOn ;
                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onDataRange xsd:dateTime
                        ] .



###  http://www.centeropenmiddleware.com/ontology/2015/scm#Commit

:Commit rdf:type owl:Class ;
        
        rdfs:subClassOf :Action .



###  http://www.centeropenmiddleware.com/ontology/2015/scm#FastForward

:FastForward rdf:type owl:Class ;
             
             rdfs:subClassOf :Merge .



###  http://www.centeropenmiddleware.com/ontology/2015/scm#Merge

:Merge rdf:type owl:Class ;
       
       rdfs:subClassOf :Action .



###  http://www.centeropenmiddleware.com/ontology/2015/scm#TrueMerge

:TrueMerge rdf:type owl:Class ;
           
           rdfs:subClassOf :Merge ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :mergeCommit ;
                             owl:onClass :Commit ;
                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
                           ] .





